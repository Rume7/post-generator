<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes>Spring Boot dependencies are managed by Spring Boot parent</notes>
        <packageUrl regex="true">pkg:maven/org\.springframework\.boot/spring-boot-starter-.*</packageUrl>
        <cve>CVE-2025-41234</cve>
        <cve>CVE-2024-22262</cve>
        <cve>CVE-2024-38809</cve>
        <cve>CVE-2024-38816</cve>
    </suppress>

    <!-- Suppress false positives for Swagger UI -->
    <suppress>
        <notes>Swagger UI vulnerabilities are in development-only dependencies</notes>
        <packageUrl regex="true">pkg:maven/org\.springdoc/springdoc-openapi-starter-webmvc-ui</packageUrl>
        <cve>CVE-2024-45801</cve>
        <cve>CVE-2024-47875</cve>
    </suppress>

    <!-- Suppress false positives for Tomcat -->
    <suppress>
        <notes>Tomcat vulnerabilities are managed by Spring Boot</notes>
        <packageUrl regex="true">pkg:maven/org\.apache\.tomcat\.embed/tomcat-embed-.*</packageUrl>
        <cve>CVE-2024-38286</cve>
        <cve>CVE-2025-24813</cve>
        <cve>CVE-2025-31651</cve>
        <cve>CVE-2025-31650</cve>
    </suppress>
</suppressions> 